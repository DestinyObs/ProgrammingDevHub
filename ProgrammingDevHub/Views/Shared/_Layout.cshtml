<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ProgrammingDevHub</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <script src='https://kit.fontawesome.com/a076d05399.js' crossorigin='anonymous'></script>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ProgrammingDevHub.styles.css" asp-append-version="true" />
</head>

<style>
    .bi {
        vertical-align: -.125em;
        fill: currentColor;
      }
      .bd-mode-toggle {
        z-index: 1500;
      }

    body {
        font-family: 'Poppins', sans-serif;
        font-weight: 400;
        line-height: 1.45;
        color: #333;
        background: #f7fafc;
    }
    </style>
<body>

        <header class="p-3 mb-3 border-bottom">
            <div class="container">
                <div class="d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start">
                    <a href="/" class="d-flex align-items-center mb-2 mb-lg-0 link-body-emphasis text-decoration-none">
                        <svg class="bi me-2" width="40" height="32" role="img" aria-label="Bootstrap"><use xlink:href="#bootstrap" /></svg>
                    </a>

                    <ul class="nav col-12 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0">
                        <li><a asp-area="" asp-controller="Home" asp-action="Index" class="nav-link px-2 link-secondary">Home</a></li>
                        <li><a asp-area="" asp-controller="Club" asp-action="Index" class="nav-link px-2 link-body-emphasis">Club</a></li>
                        <li><a asp-area="" asp-controller="MeetUp" asp-action="Index" class="nav-link px-2 link-body-emphasis">MeetUps</a></li>
                    @if (User.Identity.IsAuthenticated)
                    {
                        <li><a asp-area="" asp-controller="User" asp-action="Index" class="nav-link px-2 link-body-emphasis">More Developers</a></li>
                    }
                    </ul>

                    @*<form class="col-12 col-lg-auto mb-3 mb-lg-0 me-lg-3" role="search">
                        <input type="search" class="form-control" placeholder="Search..." aria-label="Search">
                    </form>*@

                    <div class="dropdown text-end">
                        <a href="#" class="d-block link-dark text-decoration-none dropdown-toggle" data-bs-toggle="dropdown" aria-expanded="false">
                        <img src="data:image/jpeg;base64," alt="mdo" width="32" height="32" class="rounded-circle">
                        </a>
                        @if (User.Identity.IsAuthenticated)
                        {
                        <ul class="dropdown-menu text-small">
                            <li><a asp-action="Profile" asp-controller="User" class="dropdown-item">Profile</a></li>
                        
                            <li><hr class="dropdown-divider"></li>
                            <li><a asp-action="Index" asp-controller="Dashboard" asp-area="" class="dropdown-item">Dashboard</a></li>
                            <li><a asp-action="Logout" asp-controller="Account" asp-area=""  class="dropdown-item btn-outline-danger">Logout</a></li>

                        </ul>
                        }
                        else
                        {
                            
                        <ul class="dropdown-menu text-small">
                            <li><hr class="dropdown-divider"></li>
                            <li><a asp-action="Login" asp-controller="Account" asp-area="" class="dropdown-item">Login-In</a></li>
                            <li><a asp-action="Register" asp-controller="Account" asp-area="" class="dropdown-item">Register</a></li>
                        </ul>

                        }
                    </div>
                </div>
            </div>
        </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - ProgrammingDevHub - <a asp-area="" asp-controller="Home" asp-action="Index" class="d-inline-flex link-body-emphasis text-decoration-none">
                    <i class='fas fa-code' style='font-size: 20px'></i></a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
